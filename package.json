{
  "name": "vnotes",
  "displayName": "vnotes",
  "description": "Vs code extenstion to save notes globally",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vnotes.addEntry",
        "title": "Add VNote",
        "category": "VNotes",
        "icon": "src/resources/AppIcon.svg"
      }
    ],
    "viewsWelcome": [
      {
        "view": "vnotesView",
        "contents": "No VNotes yet! Create your first VNote to get started.\n[Create New VNote](command:vnotes.addEntry)"
      }
    ],
    "views": {
      "vnotes": [
        {
          "id": "vnotesView",
          "name": ""
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "vnotes.addEntry",
          "when": "view == vnotesView"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vnotes",
          "title": "vnotes",
          "icon": "src/resources/AppIcon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
